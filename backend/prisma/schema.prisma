generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "cockroachdb"
  url      = env("DATABASE_URL")
}

model User {
  id          BigInt @id
  phoneNumber String @unique
  session     String
  notifiers   Notifier[]
}

model Notifier {
  id     String @id @default(uuid())
  chatId Int
  rule   String
  user   User   @relation(fields: [userId], references: [id])
  userId BigInt
}
